{
	"Cenario": {
		"prefix": ["Cenário", "cnr"],
		"body": [
			"Cenário: $1",
			"\tDado $2",
			"\tQuando $3",
			"\tEntão $4"
		],
		"description": "Cenário para teste"
	},
	"Esquema do Cenário": {
		"prefix": ["Esquema", "esqcnr"],
		"body": [
			"Esquema do Cenário: $1",
			"\tDado $2",
			"\tQuando $3",
			"\tEntão $4",
			"\tExemplos:",
			"\t||"
		],
		"description": "Snippets para esquema do cenário"
	},
	"Contexto": {
		"prefix": ["Contexto", "ctx"],
		"body": [
			"Contexto: $1",
			"\tDado $2"
		],
		"description": "Snippets para contexto"
	},
	"Funcionalidade": {
		"prefix": ["Funcionalidade", "fnc"],
		"body": [
			"Funcionalidade: $1",
			"\tComo $2",
			"\tGostaria $3",
			"\tPara $4"
		],
		"description": "Snippets para funcionalidade"
	},
	"Funcionalidade Completa": {
		"prefix": ["fcc"],
		"body": [
			"Funcionalidade: $1",
			"\tComo $2",
			"\tGostaria $3",
			"\tPara $4",
			"",
			"\tContexto: $5",
			"\t\tDado $6",
			"",
			"\tCenário: $7",
			"\t\tE $8",
			"\t\tQuando $9",
			"\t\tEntão $10"
		],
		"description": "Snippets para funcionalidade com contexto e cenário"
	}
}