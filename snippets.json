{
	"Cenario": {
		"prefix": ["Cenário", "cnr"],
		"body": [
			"Cenário: $1",
			"\tDado $2",
			"\tQuando $3",
			"\tEntão $4"
		],
		"description": "Cenário com steps padrões"
	},
	"Esquema do Cenário": {
		"prefix": ["Esquema", "esqcnr"],
		"body": [
			"Esquema do Cenário: $1",
			"\tDado $2",
			"\tQuando $3",
			"\tEntão $4",
			"",
			"\tExemplos:",
			"\t||"
		],
		"description": "Esquema do cenário com tabela de exemplos"
	},
	"Contexto": {
		"prefix": ["Contexto", "ctx"],
		"body": [
			"Contexto: $1",
			"\tDado $2"
		],
		"description": "Contexto com um step Dado"
	},
	"Funcionalidade": {
		"prefix": ["Funcionalidade", "fnc"],
		"body": [
			"Funcionalidade: $1",
			"\tComo $2",
			"\tGostaria $3",
			"\tPara $4"
		],
		"description": "Funcionalidade com história de usuário"
	},
	"Funcionalidade Completa": {
		"prefix": ["fcc"],
		"body": [
			"Funcionalidade: $1",
			"\tComo $2",
			"\tGostaria $3",
			"\tPara $4",
			"",
			"\tContexto: $5",
			"\t\tDado $6",
			"",
			"\tCenário: $7",
			"\t\tE $8",
			"\t\tQuando $9",
			"\t\tEntão $10"
		],
		"description": "Funcionalidade com história, contexto e cenário"
	},
	"Cenario dentro do contexto": {
		"prefix": ["cdc"],
		"body": [
			"Cenário: $1",
			"\tE $2",
			"\tQuando $3",
			"\tEntão $4"
		],
		"description": "Cenário com precondição dentro de contexto"
	},
	"Cenário sem precondição": {
		"prefix": ["csc"],
		"body": [
			"Cenário: $1",
			"\tQuando $2",
			"\tEntão $3"
		],
		"description": "Cenário sem precondição"
	}
}